{
  "expo": {
    "name": "trimathic",
    "slug": "trimathic",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "license": "MIT",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.trimathic.trimathic"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./src/assets/favicon.png"
    },
    "ios": {
      "bundleIdentifier": "com.trimathic.trimathic"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.6.10"
          }
        }
      ],
      "@morrowdigital/watermelondb-expo-plugin"
    ],
    "extra": {
      "eas": {
        "projectId": "d6fdea5a-98bb-4b04-9fe4-d918fdfb1ac6"
      }
    }
  }
}
